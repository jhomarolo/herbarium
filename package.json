{
  "name": "@herbsjs/herbarium",
  "version": "1.0.0",
  "description": "Herbs Object Repository and Discovery.",
  "main": "./src/herbarium.js",
  "scripts": {
    "test": "mocha --timeout 999999 --colors --recursive ./src",
    "lint": "npx eslint \"**/*.{js,jsx}\" --quiet",
    "lint:fix": "npx eslint \"**/*.{js,jsx}\" --fix",
    "coverage-report": "nyc --reporter=html --reporter=text mocha --recursive ./src --timeout 10000",
    "coverage": "nyc --reporter=text mocha --recursive ./src --timeout 10000",
    "semantic-release": "semantic-release",
    "commit": "cz"
  },
  "directories": {
    "lib": "src"
  },
  "bugs": {
    "url": "https://github.com/herbsjs/herbarium/issues/"
  },
  "engines": {
    "node": ">= 12.3.0"
  },
  "keywords": [
    "domain",
    "entity",
    "value object",
    "DDD",
    "clean architecture"
  ],
  "author": "David Lojudice Sb",
  "license": "MIT",
  "homepage": "https://github.com/herbsjs/herbarium#readme",
  "dependencies": {
    "require-all": "^3.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "semantic-release": "^19.0.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/herbsjs/herbarium.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
